{
  "compilerOptions": {
 "rootDir": "./",
  "outDir": "./dist",
  "module": "nodenext",
  "target": "esnext",
  "sourceMap": true,
  "declaration": true,
  "declarationMap": true,
  "strict": true,
  "jsx": "react-jsx",
  "verbatimModuleSyntax": true,
  "isolatedModules": true,
  "noUncheckedSideEffectImports": true,
  "moduleDetection": "force",
  "skipLibCheck": true
  }
}