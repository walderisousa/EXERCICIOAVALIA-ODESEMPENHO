{
  "compilerOptions": {
 "rootDir": "./",
  "outDir": "./dist",
  "module": "commonjs",
  "target": "ES2020",
  "sourceMap": true,
  "declaration": true,
  "declarationMap": true,
  "strict": true,
  "jsx": "react-jsx",
  "verbatimModuleSyntax": true,
  "isolatedModules": true,
  "noUncheckedSideEffectImports": true,
  "moduleDetection": "force",
  "skipLibCheck": true,
  "esModuleInterop": true,
  "types": ["node"]
  }
}